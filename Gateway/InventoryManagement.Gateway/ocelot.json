        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1m",
        "PeriodTimespan": 30,
        "Limit": 10
      },
      "UpstreamHttpMethod": [ "PUT", "POST" ]
    },
    // ------------------------------------ Company -------------------------------------------







